{
    "name": "backend",
    "version": "1.0.0",
    "description": "backend is the ultimate backend solution",
    "main": "src/server.js",
    "scripts": {
        "setup": "npm install && npm update",
        "tsc": "tsc -outDir build/src",
        "build": "npm run tsc",
        "start": "ts-node-dev --transpileOnly ./src/server.ts",
        "dev": "ts-node-dev --respawn --transpileOnly ./src/server.ts",
        "prod": "node ./server.js"
    },
    "keywords": [
        "express",
        "backend",
        "backend"
    ],
    "license": "UNLICENSED",
    "dependencies": {
        "@now/node-server": "^0.8.2",
        "@types/express": "^4.17.1",
        "@types/joi": "^14.3.4",
        "@types/mongoose": "^5.7.16",
        "cookie-parser": "^1.4.4",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "express-custom-error": "^1.8.4",
        "has-keys": "^0.1.0",
        "helmet": "^3.21.2",
        "http-status": "^1.3.2",
        "joi": "^14.3.1",
        "mandatoryenv": "^1.1.1",
        "module-alias": "^2.2.2",
        "mongoose": "^5.9.13",
        "morgan": "^1.9.1",
        "rotating-file-stream": "^1.4.6",
        "ts-node-dev": "^1.0.0-pre.43",
        "typescript": "^3.6.3"
    },
    "_moduleAliases": {
        "@root": "src",
        "@config": "src/config",
        "@controllers": "src/controllers",
        "@models": "src/models",
        "@util": "src/util",
        "@routes": "src/routes"
    },
    "devDependencies": {}
}
